<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss}%5.-5level [%-8.8t] %-24.24C{3} - %msg%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="j4cups" level="INFO" />
    <Logger name="j4cups.protocol.AbstractIpp" level="DEBUG" />
    <Root level="INFO">
      <AppenderRef ref="CONSOLE" />
    </Root>
  </Loggers>
</Configuration>
